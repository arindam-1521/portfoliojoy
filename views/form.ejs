<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Contact Me</title>
    <link rel="stylesheet" href="/static/css/plugin.css">
    <link rel="stylesheet" href="/static/css/main.css">
    <link rel="stylesheet" href="/static/css/responsive.css">
    <link rel="stylesheet" href="/static/css/form.css">
    <link rel="icon" type="image/x-svg" href="/static/img/logoh.svg">
</head>


<body>

    <!-- Start Preload -->
    <%- include("preloader") -%>
        <!-- End Preload -->

        <!-- Start Header -->
        <%- include('header') -%>
            <!-- End Header -->


            <!-- Start Content -->
            <div id="spiral">

                <div id="">
                    <div data-depth="0.2">
                        <div class="bg-work-3"></div>
                    </div>
                    <div data-depth="0.3">
                        <div class="bg-work-4"></div>
                    </div>
                </div>
                <div class="background1">

                    <div class="container">
                        <div class="screen" style="z-index: 100;">
                            <div class="screen-header">
                                <div class="screen-header-left">
                                    <div class="screen-header-button close1"></div>
                                    <div class="screen-header-button maximize"></div>
                                    <div class="screen-header-button minimize"></div>
                                </div>
                                <div class="screen-header-right">
                                    <div class="screen-header-ellipsis"></div>
                                    <div class="screen-header-ellipsis"></div>
                                    <div class="screen-header-ellipsis"></div>
                                </div>
                            </div>
                            <div class="screen-body">
                                <div class="screen-body-item left">
                                    <div class="app-title">
                                        <span>CONTACT</span>
                                        <span>US</span>
                                    </div>
                                    <div class="app-contact">CONTACT INFO : +62 81 314 928 595</div>
                                </div>
                                <form action="/contact" method="POST" class="screen-body-item">
                                    <div class="app-form">
                                        <div class="app-form-group">
                                            <input class="app-form-control" placeholder="NAME" name="name" type="text">
                                        </div>
                                        <div class="app-form-group">
                                            <input class="app-form-control" placeholder="AGE" name="age" type="number">
                                        </div>
                                        <div class="app-form-group">
                                            <input class="app-form-control" name="email" placeholder="EMAIL" type="email">
                                        </div>
                                        <div class="app-form-group">
                                            <input class="app-form-control" name="number" placeholder="CONTACT NO" type="number">
                                        </div>
                                        <div class="app-form-group message">
                                            <textarea class="app-form-control" name="message" placeholder="MESSAGE"></textarea>
                                        </div>
                                        <div class="app-form-group buttons">
                                            <a class="app-form-button" href="/" style="text-decoration: none;">GO BACK</a>
                                            <button class="app-form-button">&nbsp;SEND&nbsp;</button>
                                            <input class="app-form-button" type="reset" style="text-transform: uppercase;"></input>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>

                    </div>
                </div>
                <!-- Header -->
                <div class="scrolls" style="margin-top: 26px;">
                    <img draggable="false" src="/static/img/sroll.svg" alt="scroll">
                </div>
                <div class="main-head">
                    <div id="headmove">
                        <div data-depth="0.2">
                            <div class="bg-right"></div>
                        </div>
                    </div>
                    <div class="heading-text">
                        <div class="heading-text-front">
                            <h1>CONTACT US FOR, <br> MORE INFROMATION <br>AND HELP!</h1>
                        </div>
                        <div class="heading-text-back">
                            <h1>CONTACT US FOR, <br> MORE INFROMATION <br>AND HELP!</h1>
                        </div>
                    </div>
                </div>


                <!-- About -->



                <!-- Portfolio -->




                <!-- Footer -->
                <%- include ("footer") -%>


            </div>
            <!-- End Content -->

            <!-- Toggle menu -->
            <%- include("menu") -%>

                <!-- Cursor -->
                <div class="cursor1" id="cursor1"></div>
                <div class="cursor" id="cursor"></div>

                <script src="/static/js/plugin.js"></script>
                <script src="/static/js/main.js"></script>
</body>

</html>